{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "OPENCV_VIDEOIO_PRIORITY_MSMF": "0"
  },
  "functions": {
    "app.py": {
      "maxDuration": 60
    }
  }
}